{
  "author": "strekmann",
  "name": "punchy",
  "description": "project",
  "version": "0.3.0",
  "homepage": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": "6.9"
  },
  "metadata": {
    "graphql": {
      "schema": "./src/graphql/schema.json"
    }
  },
  "scripts": {
    "build": "npm run build:schema && npm run build:dist && npm run build:client",
    "build:client": "webpack --progress --colors --config webpack.prod.config.js",
    "build:dist": "babel src -d dist && babel config  -d dist/config && rm -rf dist/client",
    "build:schema": "babel-node ./scripts/build-schema.js || :",
    "dev": "webpack-dev-server --progress --colors",
    "dist": "rm -rf node_modules && rm -rf dist && mkdir dist && yarn install && cp -r node_modules dist/ && npm run build",
    "lint": "eslint src/client \"src/**/*.js\" || :",
    "nodemon": "nodemon src/index.js --exec babel-node | bunyan -o short",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "mocha test/suite.js src/**/*.spec.js || :"
  },
  "main": "dist/index.js",
  "dependencies": {
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "bunyan": "^1.8.5",
    "config": "^1.25.1",
    "connect-mongo": "^1.3.2",
    "express": "^4.14.1",
    "express-bunyan-logger": "^1.3.1",
    "express-graphql": "^0.6.3",
    "express-session": "^1.15.1",
    "graphql": "^0.9.1",
    "graphql-custom-datetype": "^0.3.0",
    "graphql-relay": "^0.5.1",
    "helmet": "^3.4.0",
    "hpp": "^0.2.1",
    "isomorphic-relay": "^0.7.4",
    "isomorphic-relay-router": "^0.8.6",
    "material-ui": "^0.17.0",
    "moment": "^2.17.1",
    "moment-duration-format": "^1.3.0",
    "mongodb": "^2.2.24",
    "mongoose": "^4.8.3",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-relay": "^0.10.0",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "relay-local-schema": "^0.5.5",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-relay-plugin": "^0.11.0",
    "babel-relay-plugin-loader": "^0.10.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.3.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.0",
